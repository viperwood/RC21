<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="RC21.AccountantWindow"
        Title="AccountantWindow">
    <Grid ColumnDefinitions="* * *" RowDefinitions="* * *">
        <StackPanel Grid.Column="1" Grid.Row="1">
            <TextBlock Text="Формирование счета страховой компании" Margin="0 10"/>
            <TextBox Watermark="Название страховой компании" Margin="0 10"/>
            <TextBox Watermark="период для оплаты" Margin="0 10"/>
            <TextBox Watermark="ФИО пациентов" Margin="0 10"/>
            <TextBox Watermark="оказанные услугами" Margin="0 10"/>
            <TextBox Watermark="стоимостью каждой услуги" Margin="0 10"/>
        </StackPanel>
    </Grid>
</Window>
